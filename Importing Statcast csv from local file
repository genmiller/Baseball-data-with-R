umpire char varying (50),
sv_id text,
vx0 numeric,
vy0 numeric,
vz0 numeric,
ax numeric,
ay numeric,
az numeric,
sz_top numeric,
sz_bot numeric,
hit_distance_sc smallint,
launch_speed numeric,
launch_angle numeric,
effective_speed numeric,
release_spin_rate smallint,
release_extension numeric,
game_pk text,
pitcher_1 text,
fielder_2_1 text,
fielder_3 text,
fielder_4 text,
fielder_5 text,
fielder_6 text,
fielder_7 text,
fielder_8 text,
fielder_9 text,
release_pos_y numeric,
estimated_ba_using_speedangle numeric,
estimated_woba_using_speedangle numeric,
woba_value numeric,
woba_denom numeric,
babip_value numeric,
iso_value numeric,
launch_speed_angle smallint,
at_bat_number smallint,
pitch_number smallint,
pitch_name char varying (50),
home_score smallint,
away_score smallint,
bat_score smallint,
fld_score smallint,
post_away_score smallint,
post_home_score smallint,
post_bat_score smallint,
post_fld_score smallint,
